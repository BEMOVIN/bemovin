{
  "name": "BEMOVIN",
  "version": "1.0.0",
  "description": "Template to build a website without a front-end framework. Includes transpilation of ES6+ JavaScript and Sass support",
  "main": "main.js",
  "author": "Esau Silva",
  "license": "MIT",
  "homepage": "https://bemovin.github.io/bemovin",
  "scripts": {
    "dev:assets": "webpack --watch",
    "dev:start": "live-server --open=./public/ --host=0.0.0.0 --watch=./public/",
    "dev": "npm-run-all -p dev:*",
    "build": "cross-env NODE_ENV=production webpack",
    "deploy": "npm run build && gh-pages -d public"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "babel-loader": "^8.0.4",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.4",
    "file-loader": "^1.1.11",
    "gh-pages": "^2.0.1",
    "live-server": "^1.2.1",
    "mini-css-extract-plugin": "^0.4.3",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.3",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^5.3.0",
    "sass-loader": "^7.1.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.1.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/esausilva/live-reload-vanilla-website-template.git"
  },
  "bugs": {
    "url": "https://github.com/esausilva/live-reload-vanilla-website-template/issues"
  },
  "keywords": [
    "JavaScript",
    "Webpack",
    "ES6+",
    "live reload",
    "Sass"
  ],
  "dependencies": {
    "aos": "^3.0.0-beta.6",
    "bootstrap": "^4.3.1",
    "jquery": "^3.4.1",
    "popper.js": "^1.14.5"
  }
}
